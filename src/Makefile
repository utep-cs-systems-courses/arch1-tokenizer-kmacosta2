#defining variable $(OBJS) to list of object files
OBJS = UImain.o tokenizer.o history.o


# building the object file thru CFLAGS

CFLAGS=-g -O3 # -g for debug, -03 for optimization

#first target build by default
all:UImain

# (target) depends on all object files (prerequisites)
UImain: $(OBJS)
	cc -o UImain $(CFLAGS) $(OBJS)

# all objects files built from c files
$OBJS: tokenizer.h history.h

# deletes files generated by compilation
clean:
	rm -f *.o UImain

run:UImain
	./UImain
#later im going to need to compile a history.c
